@page
@model ForeningsPortalen.Website.Pages.Bookings.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].CreatedOndate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].CategoryName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].BookingUnits)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingIndexModel[0].Rowversion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.BookingIndexModel) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOndate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BookingUnits)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rowversion)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
